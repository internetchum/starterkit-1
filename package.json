{
  "name": "GulpPackageManager",
  "version": "0.0.0",
  "description": "Gulp Package Manager Starter",
  "main": "gulpfile.js",
  "author": "Fordyce Gozali",
  "readMeFile": "README.md",
  "browser": {
    "underscore": "backbone/node_modules/underscore",
    "jquery": "./public/plugins/jquery.min.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "coffeeify",
      "hbsfy"
    ]
  },
  "browserify-shim": {
    "plugin": {
      "exports": "plugin",
      "depends": [
        "jquery:$"
      ]
    }
  },
  "devDependencies": {
    "browserify": "~5.10.1",
    "browserify-shim": "~3.7.0",
    "coffeeify": "^0.7.0",
    "gulp": "^3.8.7",
    "gulp-changed": "^0.4.1",
    "gulp-clean": "^0.3.1",
    "gulp-coffee": "^2.2.0",
    "gulp-copy": "0.0.2",
    "gulp-htmlmin": "^0.2.0",
    "gulp-imagemin": "^0.6.2",
    "gulp-minify-css": "^0.3.10",
    "gulp-notify": "^1.4.2",
    "gulp-plumber": "^0.6.5",
    "gulp-sass": "^0.7.3",
    "gulp-uglify": "^1.0.1",
    "gulp-util": "^3.0.0",
    "handlebars": "^1.3.0",
    "hbsfy": "^2.2.0",
    "pretty-hrtime": "~0.2.1",
    "require-dir": "^0.1.0",
    "run-sequence": "^0.3.7",
    "underscore": "^1.7.0",
    "vinyl-source-stream": "^0.1.1",
    "watchify": "^1.0.6"
  },
  "dependencies": {
    "backbone": "~1.1.2",
    "jquery": "~2.1.0"
  }
}
